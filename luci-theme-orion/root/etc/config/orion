	option name 'Overview'
	option enable 'Enable'
	option line '1'
	option newtab 'No'
	option icon 'resources/icons/navbar/overview.png'
	option address '/cgi-bin/luci/admin/status/overview'

config navbar
	option name 'Terminal'
	option enable 'Enable'
	option line '2'
	option newtab 'No'
	option icon 'resources/icons/navbar/terminal.png'
	option address '/cgi-bin/luci/admin/services/ttyd'

